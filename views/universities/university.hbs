                        <div class="row">
                            <div class="col-lg-4">
                                <div class="col-12">
                                    <div class="widget-rounded-circle card-box">
                                        <div class="row align-items-center">
                                            <div class="col-auto">
                                                <div class="avatar-lg">
                                                    <img src="{{universityData.logoUrl}}" class="img-fluid" alt="user-img" />
                                                </div>
                                            </div>
                                            <div class="col">
                                                <h5 class="mb-1 mt-2">{{universityData.name}}</h5>
                                                <p class="mb-2 text-muted">
                                                    <a href="{{parseUrl 'set-university-logo' urls id=universityData._id}}" class="card-link text-custom">Change Logo</a>
                                                </p>
                                            </div>
                                        </div> <!-- end row-->
                                    </div> <!-- end widget-rounded-circle-->
                                </div> <!-- end col-->
                                <input type="hidden" id="libSearchUrl" value="{{parseUrl 'add-librarian-search' urls}}">
                                <input type="hidden" id="addLibUrl" value="{{parseUrl 'add-librarian' urls id=universityData._id user='toBeReplaced'}}">
                                <input type="hidden" id="removeLibUrl" value="{{parseUrl 'remove-librarian' @root.urls id=universityData._id user='toBeReplaced'}}">


                                <div class="col-12">
                                    <div class="widget-rounded-circle card-box" id="librarian">
                                        {{#unless universityData.librarian}}
                                            <div class="row">
                                                <div class="col">
                                                    <input type="text" class="form-control chat-input" id="librarianSearchBox" placeholder="Search Librarian...">
                                                    
                                                    <div class="search-view-container">
                                                        <ul class="search-view" id="libSearchResults"></ul>
                                                    </div>
                                                </div>
                                            </div>
                                        {{else}}
                                        <div class="row align-items-center">
                                            <div class="col-auto">
                                                <div class="avatar-lg">
                                                    <img src="{{universityData.librarian.photoUrl}}" class="img-fluid rounded-circle" alt="user-img" />
                                                </div>
                                            </div>
                                            <div class="col">
                                                <h5 class="mb-1 mt-2">{{universityData.librarian.fullName}}</h5>
                                                <a href="#" onclick="removeLibrarian('{{universityData.librarian._id}}')" class="card-link text-custom" style="color: red;">Remove</a>
                                            </div>
                                        </div> <!-- end row-->
                                        {{/unless}}
                                    </div> <!-- end widget-rounded-circle-->
                                </div> <!-- end col-->
                                
                                
                                <!-- University-Information -->
                                <div class="col-12">
                                    <div class="card-box">
                                        <div class="row">
                                            <div class="col-6">
                                                <div class="avatar-sm rounded">
                                                    <i class="fe-alert-circle avatar-title font-22" style="color: #c5c5c5;"></i>
                                                </div>
                                            </div>
                                            <div class="col-6">
                                                <div class="text-right">
                                                    <a class="btn btn-danger waves-effect waves-light btn-sm" href="#">
                                                        <i class="mdi mdi-square-edit-outline avatar-title text-white"></i>
                                                    </a>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="mt-3">
                                            <div class="text-left">
                                                <p class="text-muted"><strong>Name :</strong> <span class="ml-2">{{universityData.name}}</span></p>

                                                {{#if universityData.phoneNumber}}
                                                    <p class="text-muted"><strong>Mobile :</strong><span class="ml-2">{{universityData.phoneNumber}}</span></p>
                                                {{/if}}

                                                <p class="text-muted"><strong>Email :</strong> <span class="ml-2">{{universityData.email}}</span></p>

                                                {{#if universityData.address}}
                                                    <p class="text-muted"><strong>Address :</strong><span class="ml-2">{{universityData.address}}</span></p>
                                                {{/if}}

                                                {{#if universityData.gender}}
                                                    <p class="text-muted"><strong>Gender :</strong><span class="ml-2">{{universityData.gender}}</span></p>
                                                {{/if}}

                                                {{#if universityData.dateOfBirth}}
                                                    <p class="text-muted"><strong>Date of birth :</strong><span class="ml-2">{{formatDate universityData.dateOfBirth}}</span></p>
                                                {{/if}}

                                                {{#if universityData.country}}
                                                    <p class="text-muted"><strong>Country :</strong><span class="ml-2">{{universityData.country}}</span></p>
                                                {{/if}}
                                            </div>
                                        </div> <!-- end card-box-->
                                    </div>
                                </div> <!-- end col -->

                            <div class="col-lg-8">
                            </div>
                            <!-- end col -->
                        </div>
                        <!-- end row -->