<div class="row">
    <div class="col-xl-4 col-lg-12">
        <div class="card">
            <div class="card-body">
                <div class="dropdown float-right">
                    <a href="#" class="dropdown-toggle arrow-none card-drop" data-toggle="dropdown" aria-expanded="false">
                        <i class="mdi mdi-dots-vertical"></i>
                    </a>
                    <div class="dropdown-menu dropdown-menu-right">
                        <!-- item-->
                        
                            <a href="{{parseUrl 'populate-data-module' urls}}" class="dropdown-item">{{#isEmpty docs}}Init menu{{else}}Refresh Menu{{/isEmpty}}</a>
                        <!-- item-->
                        <a href="javascript:void(0);" class="dropdown-item">Action</a>
                    </div>
                </div>
                <h4 class="header-title mb-3">Data module menu items</h4>

                <div class="table-responsive">
                    <table class="table table-striped table-sm table-nowrap table-centered mb-0">
                        <thead>
                            <tr>
                                <th>Menu</th>
                                <th>Module</th>
                                <th>Active</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            {{#each docs}}
                            <tr>
                                <td>
                                    <h5 class="font-15 mb-1 font-weight-normal">{{this.title}}</h5>
                                </td>
                                <td>{{this.module}}</td>
                                <td>{{#if this.active}}<i class="uil uil-check" style="font-size: 18px;"></i>{{/if}}</td>
                                <td class="table-action">
                                    <a href="javascript: void(0);" class="action-icon"> <i class="mdi mdi-delete"></i></a>
                                </td>
                            </tr>
                            {{/each}}
                        </tbody>
                    </table>
                </div> <!-- end table-responsive-->

            </div> <!-- end card-body-->
        </div> <!-- end card-->
    </div>
    <!-- end col-->
</div>